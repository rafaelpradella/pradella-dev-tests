    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "jest-vue": "^0.8.2",
    "msw": "^0.23.0",
    "node-sass": "^5.0.0",
